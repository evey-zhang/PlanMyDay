<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserApplicationLayer.AttractionDetails">

	<RelativeLayout
		android:id="@+id/attraction_detail"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:background="#FFECEC"
		android:clipToOutline="true"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.0">


		<ImageView
			android:id="@+id/ellipse"
			android:layout_width="match_parent"
			android:layout_height="386.14dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="2dp"
			android:layout_marginTop="-116dp"
			android:scaleType="centerCrop"
			android:src="@drawable/ellipse_2" />

		<TextView
			android:layout_width="159dp"
			android:layout_height="25dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="45dp"
			android:layout_marginTop="70dp"
			android:gravity="top"
			android:text="More Details"
			android:textColor="#000000"
			android:textSize="15sp"
			android:textStyle="bold" />

		<View
			android:layout_width="72dp"
			android:layout_height="3dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="45dp"
			android:layout_marginTop="95dp"
			android:background="@drawable/line_3"
			android:textColor="#000000"
			android:textSize="12sp" />
		<ImageButton
			android:id="@+id/backButton"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"

			app:srcCompat="?attr/homeAsUpIndicator" />


		<androidx.cardview.widget.CardView
			android:id="@+id/rectangle_1"
			android:layout_width="330dp"
			android:layout_height="120dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="39dp"
			android:layout_marginTop="150dp"
			android:background="#FEFEFE"
			android:elevation="0dp"
			app:cardCornerRadius="10dp">

			<FrameLayout
				android:id="@+id/component_container"
				android:layout_width="328dp"
				android:layout_height="120dp"
				android:background="#FEFEFE"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<TextView
					android:id="@+id/cell_time"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="15dp"
					android:background="#EFABAB"
					android:padding="8dp"
					android:text="7:00am - 8:00pm"
					android:textColor="#FEFEFE"
					android:textSize="8sp" />

				<TextView
					android:id="@+id/cell_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="56dp"
					android:gravity="start"
					android:text="USC Dulce"
					android:textColor="#000000"
					android:textSize="13sp"
					android:textStyle="bold" />

				<View
					android:id="@+id/line_4"
					android:layout_width="90dp"
					android:layout_height="2dp"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="76dp"
					android:background="@drawable/line_3" />

				<TextView
					android:id="@+id/cell_address"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="86dp"
					android:gravity="start"
					android:text="USC Village, 90007\n"
					android:textColor="#000000"
					android:textSize="10sp" />

			</FrameLayout>


		</androidx.cardview.widget.CardView>

		<!-- Auto layout, variables, and unit scale are not yet supported -->

		<!-- Auto layout, variables, and unit scale are not yet supported -->
		<androidx.cardview.widget.CardView
			android:layout_width="331dp"
			android:layout_height="222dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="38dp"
			android:layout_marginTop="303dp"
			android:background="#FEFEFE"
			app:cardCornerRadius="10dp">

			<TextView
				android:layout_width="159dp"
				android:layout_height="25dp"
				android:layout_alignParentLeft="true"
				android:layout_alignParentTop="true"
				android:layout_marginLeft="23dp"
				android:layout_marginTop="11dp"
				android:elevation="4dp"
				android:gravity="top"
				android:text="Description"
				android:textColor="#000000"
				android:textSize="13sp"
				android:textStyle="bold" />

			<View
				android:layout_width="72dp"
				android:layout_height="2dp"
				android:layout_alignParentLeft="true"
				android:layout_alignParentTop="true"
				android:layout_marginLeft="23dp"
				android:layout_marginTop="31dp"
				android:background="@drawable/line_3" />

			<TextView
				android:id="@+id/description_text"
				android:layout_width="210dp"
				android:layout_height="146dp"
				android:layout_alignParentLeft="true"
				android:layout_alignParentTop="true"
				android:layout_marginLeft="29dp"
				android:layout_marginTop="44dp"
				android:gravity="top"
				android:text="blah_blah_b"
				android:textColor="#000000"
				android:textSize="11sp" />



		</androidx.cardview.widget.CardView>

		<Button
			android:id="@+id/save_button"
			android:layout_width="118dp"
			android:layout_height="44dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="140dp"
			android:layout_marginTop="550dp"
			android:backgroundTint="#EFABAB"
			android:elevation="4dp"
			android:text="Save" />

		<Button
			android:id="@+id/remove_button"
			android:layout_width="118dp"
			android:layout_height="44dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentRight="true"
			android:layout_marginTop="605dp"
			android:layout_marginRight="153dp"
			android:backgroundTint="#EFABAB"
			android:elevation="4dp"
			android:text="Remove" />


	</RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
