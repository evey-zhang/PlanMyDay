<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserApplicationLayer.AttractionDetails">

	<RelativeLayout
		android:id="@+id/attraction_detail"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:background="#FFECEC"
		android:clipToOutline="true"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.0">

		<ImageView
			android:id="@+id/ellipse"
			android:layout_width="match_parent"
			android:layout_height="386.14dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="2dp"
			android:layout_marginTop="-116dp"
			android:scaleType="centerCrop"
			android:src="@drawable/ellipse_2" />

		<TextView
			android:layout_width="159dp"
			android:layout_height="25dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="45dp"
			android:layout_marginTop="70dp"
			android:gravity="top"
			android:text="More Details"
			android:textColor="#000000"
			android:textSize="15sp"
			android:textStyle="bold" />

		<View
			android:layout_width="72dp"
			android:layout_height="3dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="45dp"
			android:layout_marginTop="95dp"
			android:background="@drawable/line_3"
			android:textColor="#000000"
			android:textSize="12sp" />

		<androidx.cardview.widget.CardView
			android:id="@+id/rectangle_1"
			android:layout_width="330dp"
			android:layout_height="120dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="39dp"
			android:layout_marginTop="150dp"
			android:background="#FEFEFE"
			app:cardCornerRadius="10dp"
			android:elevation="0dp">

			<FrameLayout
				android:id="@+id/component_container"
				android:layout_width="328dp"
				android:layout_height="120dp"
				android:background="#FEFEFE"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<TextView
					android:id="@+id/cell_time"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="15dp"
					android:background="#EFABAB"
					android:padding="8dp"
					android:text="7:00am - 8:00pm"
					android:textColor="#FEFEFE"
					android:textSize="8sp" />

				<TextView
					android:id="@+id/cell_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="56dp"
					android:gravity="start"
					android:text="USC Dulce"
					android:textColor="#000000"
					android:textSize="13sp"
					android:textStyle="bold" />

				<View
					android:id="@+id/line_4"
					android:layout_width="90dp"
					android:layout_height="2dp"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="76dp"
					android:background="@drawable/line_3" />

				<TextView
					android:id="@+id/cell_address"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="start|top"
					android:layout_marginStart="16dp"
					android:layout_marginTop="86dp"
					android:gravity="start"
					android:text="USC Village, 90007\n"
					android:textColor="#000000"
					android:textSize="10sp" />

			</FrameLayout>

		</androidx.cardview.widget.CardView>

		<androidx.cardview.widget.CardView
			android:layout_width="330dp"
			android:layout_height="263dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:layout_marginLeft="39dp"
			android:layout_marginTop="303dp"
			app:cardCornerRadius="10dp"
			android:background="#FEFEFE">

			<TextView
				android:layout_width="159dp"
				android:layout_height="25dp"
				android:layout_alignParentLeft="true"
				android:layout_alignParentTop="true"
				android:layout_marginLeft="23dp"
				android:layout_marginTop="11dp"
				android:elevation="4dp"
				android:gravity="top"
				android:text="Description"
				android:textColor="#000000"
				android:textStyle="bold"
				android:textSize="13sp" />

			<View
				android:layout_width="72dp"
				android:layout_height="2dp"
				android:layout_alignParentLeft="true"
				android:layout_alignParentTop="true"
				android:layout_marginLeft="23dp"
				android:layout_marginTop="31dp"
				android:background="@drawable/line_3" />

			<TextView
				android:id="@+id/description_text"
				android:layout_width="210dp"
				android:layout_height="196dp"
				android:layout_alignParentLeft="true"
				android:layout_marginLeft="29dp"
				android:layout_alignParentTop="true"
				android:layout_marginTop="44dp"
				android:text="blah_blah_b"
				android:textSize="11sp"
				android:textColor="#000000"
				android:gravity="top"
				/>


		</androidx.cardview.widget.CardView>

	</RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
